@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="~/Scripts/moment.min.js"></script>
<script type="text/javascript" src="~/App/Services/LuckyService.js"></script>
<script type="text/javascript" src="~/App/MyAdvertismentsModule.js"></script>

<table class="table table-striped table-hover ">
    <thead>
    <tr>
        <th>Name</th>
        <th>Finish Date</th>
        <th>Days Left</th>
        <th>Count participants</th>
        @*<th>Status</th>*@
    </tr>
    </thead>
    <tbody data-bind="foreach: myAdvs">
    <tr>
        <td><a data-bind="text: Name, attr: {href: '/Lucky/Edit/'+Id}"></a></td>
        <td data-bind="text: moment(EndDate).format('MM/DD/YYYY')"></td>
        <td data-bind="text: DaysLeft"></td>
        <td data-bind="text: ParticipationsCount"></td>
        @*<td data-bind="text: Status"></td>*@
    </tr>
    </tbody>
</table>
<script type="text/javascript">
	$(document).ready(function () {
		Window.App.MyAdvertismentsModule.init();
	});
</script>